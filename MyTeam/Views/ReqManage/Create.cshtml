        $("#PlanRlsDate").attr({ class: "form-control Wdate datepicker", onclick: "WdatePicker({ dateFmt: 'yyyy/MM/dd' })", readonly: "readonly", placeholder: "点击选择日期" });
        $("#RlsDate").attr({ class: "form-control Wdate datepicker", onclick: "WdatePicker({ dateFmt: 'yyyy/MM/dd' })", readonly: "readonly", placeholder: "点击选择日期" });
    });
</script>

